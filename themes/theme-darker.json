    "syntaxFunction": {
      "dark": "malibu",
      "light": "malibu"
    },
    "syntaxVariable": {
      "dark": "coral",
      "light": "coral"
    },
    "syntaxString": {
      "dark": "green",
      "light": "green"
    },
    "syntaxNumber": {
      "dark": "whiskey",
      "light": "whiskey"
    },
    "syntaxType": {
      "dark": "chalky",
      "light": "chalky"
    },
    "syntaxOperator": {
      "dark": "fountainBlue",
      "light": "fountainBlue"
    },
    "syntaxPunctuation": {
      "dark": "fg",
      "light": "fg"
    }
