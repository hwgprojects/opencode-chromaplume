{
  "$schema": "https://opencode.ai/theme.json",
  "defs": {
    "bg": "#23272e",
    "bgPanel": "#1e2227",
    "bgElement": "#21252b",
    "fg": "#abb2bf",
    "fgMuted": "#7f848e",
    "fgDark": "#5c6370",
    "border": "#3e4451",
    "borderSubtle": "#2c313a",
    "malibu": "#61afef",
    "fountainBlue": "#2bbac5",
    "purple": "#d55fde",
    "coral": "#ef596f",
    "chalky": "#e5c07b",
    "green": "#89ca78",
    "whiskey": "#d19a66"
