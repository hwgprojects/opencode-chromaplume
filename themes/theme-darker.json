  "theme": {
    "primary": {
      "dark": "malibu",
      "light": "malibu"
    },
    "secondary": {
      "dark": "fountainBlue",
      "light": "fountainBlue"
    },
    "accent": {
      "dark": "purple",
      "light": "purple"
    },
    "error": {
      "dark": "coral",
      "light": "coral"
    },
    "warning": {
      "dark": "chalky",
      "light": "chalky"
